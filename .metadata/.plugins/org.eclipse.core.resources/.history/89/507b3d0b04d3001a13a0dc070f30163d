
public class p160_exam2 {

	public static void main(String[] args) {
		int o = 0;
		for(int i = 1; i <= 100; i++) {
			if(i %3 == 0) {
				o += i;
				
			}
		}System.out.println("3의배수 합"+o);

		
		for (int x = 0; x <=10; x++) {
			for (int y = 0; y <=10; y++) {
				System.out.println((4*x)+(5*y));
			}
		}
	}

}
