
public class Ex1 {

	public static void main(String[] args) {
		//while 문을 사용한 구구단
		int dan = 2; //단
		int soo = 1; //초기식
		System.out.println("<"+dan+"단>");
		while (soo <= 9) {
				System.out.println(dan + "*" + soo + "=" +(dan*soo));
				soo++;
			}
		System.out.println("--------------------------------------");
		int i = 1;
		while(i<=10) {
			System.out.println(i);
			i++;
		}
		
		
	}

}
