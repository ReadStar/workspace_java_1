
public class Ex4 {

	public static void main(String[] args) {
		/*
		 * 대입연산자(=)
		 * -우변의 피연산자 데이터를 좌변의 변수에 대입(저장)
		 * 
		 * 확장(복합) 대입연산자(+=, -=, *=, /=, %=)
		 * -산술연산자와 대입연산자를 결합한 연산자
		 * -두 피연산자의 산술 연산 결과를 좌변의 변수에 대입
		 *
		 */
		int a = 10;
		a = a + 10; //a + 10의 연산 결과를 다시 변수 a에 저장
		System.out.println(a);
		//a - 10 의 연산 결과를 다시 변수 a에 저장
		a = a-10;
		System.out.println(a);
		
		a = a*10;
		System.out.println(a);
		
		a = a/;
		System.out.println(a);
		
		a = a-%;
		System.out.println(a);

	}

}
