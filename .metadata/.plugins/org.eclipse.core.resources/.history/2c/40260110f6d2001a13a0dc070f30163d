
public class Ex4 {

	public static void main(String[] args) {
		
		for (int i = 1; i <= 10; i++) {
			
		}
		
		System.out.println("----------------------------------");
		
		int i = 1;
		while(i<=10) {//i가 10보다 클 경우 반복문 내부는 실행되지 못함.
			System.out.println(i + "Hello,World");
			i++;
		}
		System.out.println("반복문 종료 후 i 값" + i);

	}

}
